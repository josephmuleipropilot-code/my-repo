from math import sin ,cos, tan ,sqrt


class EquationsofMotion:
    def __init__(self):
        self.intial_velocity = input("What is your intial velocity. \n ------ Enter your number, if the variable is missing then enter None ").strip()
        self.time = input("What is your time, \n ------ Enter your number, if the variable is missing then enter None ").strip()
        self.gravity = input("What is your gravity, \n ------ Enter your number, if the variable is missing then enter None ").strip()
        self.final_velocity = input("What is your final velocity, \n ------ Enter your number, if the variable is missing then enter None ").strip()
        self.acceleration = input("What is your acceleartion,  \n ------ Enter your number, if the variable is missing then enter None ").strip()
        self.distance = input("what is your distance,  \n ------ Enter your number, if the variable is missing then enter None ").strip()

    
    def equation_1(self):
        if self.final_velocity == None:
            return self.intial_velocity + self.acceleration * self.time
        
    
 
answer1 = EquationsofMotion()